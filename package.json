{
	"name": "crm-project",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"build": "tsc && vite build",
		"compile": "tsc && vite build",
		"preview": "vite preview",
		"test": "jest --watchAll",
		"lint": "eslint src/**/*.{ts,tsx,json,jsx,js}",
		"lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc --ignore-path vite-env.d.ts",
